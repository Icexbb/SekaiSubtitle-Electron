{
  "name": "sekaisubtitle-electron",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "",
  "author": "XBB <icexb_baka@163.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.3.3",
    "@vicons/antd": "^0.12.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fa": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons4": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^24.2.0",
    "electron-builder": "^23.6.0",
    "esbuild": "^0.17.18",
    "naive-ui": "^2.34.3",
    "sass": "^1.32.12",
    "typescript": "^5.0.2",
    "vfonts": "^0.0.3",
    "vite": "^4.1.4",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.3",
    "axios": "^1.4.0",
    "font-list": "^1.4.5",
    "highlight": "^0.2.4",
    "pinia": "^2.0.35",
    "quasar": "^2.12.0",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.1.6"
  },
  "mac": {
    "icon": "public/favicon.icns",
    "artifactName": "${productName}_setup_${version}.${ext}",
    "target": "dmg",
    "extraResources": {
      "from": "./extra/core.bin",
      "to": "../resource/core.bin"
    }
  },
  "win": {
    "icon": "public/favicon.ico",
    "artifactName": "${productName}_setup_${version}.${ext}",
    "extraResources": {
      "from": "./extra/core.exe",
      "to": "../resource/core.exe"
    }
  },
  "linux": {
    "icon": "public",
    "artifactName": "${productName}_setup_${version}.${ext}"
  },
  "extraResources": [
    {
      "from": "./extra/font-list/**",
      "to": "../resources/font-list/**"
    }
  ]
}
